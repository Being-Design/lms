#: lib/view/WpProQuiz_View_QuizEdit.php:609
#, php-format
msgid ""
"(Will be displayed, when result-percent is >= <span class=\"resultProzent\">"
"%s</span>%%)"
msgstr ""
"(уровень будет отображаться, если результат >= <span class=\"resultProzent\">"
"%s</span>%%)"

#: lib/view/WpProQuiz_View_QuizEdit.php:599
#: lib/view/WpProQuiz_View_QuizEdit.php:610
msgid "Delete graduation"
msgstr "Удалить уровень"

#: lib/view/WpProQuiz_View_QuizEdit.php:617
msgid "Add graduation"
msgstr "Добавить уровень"

